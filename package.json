{
  "name": "@usefui/create-fui-app",
  "version": "1.0.4",
  "description": "Open Source Command Line Interfaces to start app using foundation-ui technologies",
  "main": "dist/index.js",
  "bin": {
    "create-fui-app": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist",
    "cli:run": "ts-node src/index.ts",
    "cli:watch": "nodemon --exec ts-node src/index.ts"
  },
  "keywords": [
    "cli",
    "react",
    "cli",
    "typescript",
    "jsx",
    "reactjs",
    "cli",
    "generator",
    "template",
    "scaffold"
  ],
  "author": "Nicolas Nunes",
  "bugs": {
    "url": "https://github.com/foundation-ui/create-fui-app/issues"
  },
  "homepage": "https://github.com/foundation-ui/create-fui-app#readme",
  "license": "MIT",
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.3.1",
    "inquirer": "^9.3.7",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.2.0",
    "@types/ora": "^3.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "packageManager": "pnpm@10.10.0"
}
